{
  "name": "Sea of Thieves Dashboard",
  "manifest_version": 3,
  "version": "1.5.1",
  "description": "A Sea of Thieves unofficial dashboard. Must be connected to https://www.seaofthieves.com for this extension to work.",
  "permissions": [
    "tabs",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback"
  ],
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": ["https://www.seaofthieves.com/*"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Sea of Thieves Dashboard"
  }
}
